{
  "name": "@intlayer/backend",
  "version": "7.0.2",
  "private": false,
  "description": "Intlayer Backend is a an application that allow you to manage your Intlayer content and interact with the intlayer editor.",
  "keywords": [
    "intlayer",
    "multilingual",
    "i18n",
    "internationalization",
    "backend",
    "types",
    "api"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/export.d.ts",
      "require": "./dist/cjs/export.cjs",
      "import": "./dist/esm/export.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/export.cjs",
  "module": "dist/esm/export.mjs",
  "types": "dist/types/export.d.ts",
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "build:ci": "tsdown --config tsdown.config.ts",
    "clean": "rimraf ./dist .turbo",
    "dev": "node --watch ./dist/esm/index.mjs",
    "dev:package": "tsdown --config tsdown.config.ts --watch",
    "email": "email dev --dir ./src/emails",
    "format": "biome format . --check",
    "format:fix": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "prepublish": "cp -f ../../README.md ./README.md",
    "start": "cross-env NODE_ENV=production node dist/esm/index.mjs",
    "start:stripe": "stripe listen --forward-to localhost:3100/webhook/stripe",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.types.json"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.27",
    "@ai-sdk/deepseek": "1.0.23",
    "@ai-sdk/google": "2.0.23",
    "@ai-sdk/mistral": "2.0.19",
    "@ai-sdk/openai": "2.0.52",
    "@react-email/components": "0.5.6",
    "ai": "5.0.71",
    "better-auth": "1.3.29",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cross-env": "10.1.0",
    "dotenv": "16.6.1",
    "express": "5.1.0",
    "express-intlayer": "workspace:*",
    "express-rate-limit": "8.1.0",
    "helmet": "8.1.0",
    "mongodb": "6.20.0",
    "mongoose": "8.19.1",
    "oauth2-server": "3.1.1",
    "openai": "6.3.0",
    "react": "19.2.0",
    "react-email": "4.3.0",
    "resend": "6.1.3",
    "stripe": "19.1.0",
    "validator": "13.15.15",
    "winston": "3.18.3"
  },
  "devDependencies": {
    "@intlayer/config": "workspace:*",
    "@intlayer/core": "workspace:*",
    "@intlayer/docs": "workspace:*",
    "@intlayer/types": "workspace:*",
    "@types/body-parser": "1.19.6",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/crypto-js": "4.2.2",
    "@types/csurf": "1.11.5",
    "@types/express": "5.0.5",
    "@types/node": "24.9.2",
    "@types/oauth2-server": "3.0.18",
    "@types/react": "19.2.2",
    "@types/validator": "13.12.2",
    "@utils/ts-config": "workspace:*",
    "@utils/ts-config-types": "workspace:*",
    "@utils/tsdown-config": "workspace:*",
    "esbuild-fix-imports-plugin": "1.0.22",
    "intlayer": "workspace:*",
    "rimraf": "6.0.1",
    "tsdown": "0.15.11",
    "typescript": "5.9.3",
    "vitest": "4.0.5"
  }
}
