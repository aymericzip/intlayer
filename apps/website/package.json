  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "sideEffects": false,
  "scripts": {
    "build": "next build --turbopack",
    "build:ci": "next build --turbopack",
    "build:webpack": "next build",
    "dev": "intlayer watch --with 'next dev --turbopack'",
    "dev:webpack": "next dev",
    "editor": "intlayer-editor start",
    "generate-pwa-assets": "pwa-asset-generator --preset minimal ../../packages/@intlayer/design-system/src/components/Logo/logo_circle_wite.svg ./public/assets/",
    "prepare": "husky",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "serve": "npx intlayer serve",
    "start": "next start",
    "start:editor": "npx intlayer-editor start --with 'next dev --turbopack'",
    "test": "vitest run"
  },
  "dependencies": {
    "@intlayer/api": "workspace:*",
