{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "noEmit": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "types": ["react/canary", "node"]
  },
  "exclude": ["node_modules", "dist"],
  "include": ["**/*"]
}
