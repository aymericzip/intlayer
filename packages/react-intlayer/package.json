{
  "name": "react-intlayer",
  "version": "3.3.2",
  "private": false,
  "description": "Internationalization layer for React applications. Declare your multilingual contant in the same lever than your component. Powered by TypeScript, declaration files.",
  "keywords": [
    "intlayer",
    "data",
    "internationalization",
    "multilingual",
    "i18n",
    "typescript",
    "react",
    "vite",
    "json"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "require": "./dist/cjs/server/index.cjs",
      "import": "./dist/esm/server/index.mjs"
    },
    "./craco-plugin": {
      "types": "./dist/types/craco/intlayerCracoPlugin.d.ts",
      "require": "./dist/cjs/craco/intlayerCracoPlugin.cjs",
      "import": "./dist/esm/craco/intlayerCracoPlugin.mjs"
    },
    "./craco-config": {
      "types": "./dist/types/craco/craco.config.d.ts",
      "require": "./dist/cjs/craco/craco.config.cjs",
      "import": "./dist/esm/craco/craco.config.mjs"
    },
    "./vite-plugin": {
      "types": "./dist/types/vite/intlayerPlugin.d.ts",
      "require": "./dist/cjs/vite/intlayerPlugin.cjs",
      "import": "./dist/esm/vite/intlayerPlugin.mjs"
    },
    "./editor": {
      "types": "./dist/types/editor/index.d.ts",
      "require": "./dist/cjs/editor/index.cjs",
      "import": "./dist/esm/editor/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ]
    }
  },
  "bin": {
    "react-intlayer": "./dist/cjs/cli/react-intlayer.cjs"
  },
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "pnpm build:package & pnpm build:types",
    "build:package": "tsup",
    "build:types": "tsc --project ./tsconfig.types.json && tsc-alias --project ./tsconfig.types.json",
    "clean": "rimraf ./dist",
    "dev": "concurrently --prefix none \"tsup --watch\" \"tsc --project ./tsconfig.types.json --watch\" \"tsc-alias --project ./tsconfig.types.json --watch\"",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "",
    "typecheck": "tsup --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@intlayer/chokidar": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@intlayer/webpack": "workspace:^",
    "cross-spawn": "^7.0.6",
    "js-cookie": "^3.0.5",
    "react-cookie": "^7.2.2",
    "vite": "^5.4.11",
    "webpack": "^5.96.1"
  },
  "devDependencies": {
    "@craco/types": "^7.1.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@utils/eslint-config": "workspace:^",
    "@utils/ts-config": "workspace:^",
    "@utils/ts-config-types": "workspace:^",
    "@utils/tsup-config": "workspace:^",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@intlayer/chokidar": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@intlayer/dictionaries-entry": "workspace:^",
    "@intlayer/webpack": "workspace:^",
    "intlayer": "workspace:^",
    "react": ">=16.0.0 <19.0.0",
    "react-dom": ">=16.0.0 <19.0.0"
  },
  "engines": {
    "node": ">=14.18"
  },
  "bug": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  }
}
