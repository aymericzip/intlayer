{
	"name": "svelte-intlayer",
	"version": "6.1.5",
	"description": "Easily internationalize i18n your Svelte applications with type-safe multilingual content management.",
	"keywords": [
		"intlayer",
		"data",
		"internationalization",
		"multilingual",
		"i18n",
		"typescript",
		"svelte"
	],
	"homepage": "https://intlayer.org",
	"bugs": {
		"url": "https://github.com/aymericzip/intlayer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aymericzip/intlayer.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Kalu Victor",
		"url": "https://github.com/victorbuikemdev"
	},
	"contributors": [
		{
			"name": "Aymeric Pineau",
			"email": "ay.pineau@gmail.com",
			"url": "https://github.com/aymericzip"
		},
		{
			"name": "Kalu Victor",
			"email": "victorbuikemdev@gmail.com",
			"url": "https://github.com/victorbuikemdev"
		}
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"@intlayer/api": "workspace:*",
		"@intlayer/config": "workspace:*",
		"@intlayer/core": "workspace:*",
		"@intlayer/editor": "workspace:*",
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@intlayer/api": "workspace:*",
		"@intlayer/config": "workspace:*",
		"@intlayer/core": "workspace:*",
		"@intlayer/editor": "workspace:*",
		"js-cookie": "^3.0.5"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/node": "^22",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	}
}
