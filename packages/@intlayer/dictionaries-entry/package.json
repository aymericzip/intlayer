{
  "name": "@intlayer/dictionaries-entry",
  "version": "0.0.0",
  "private": false,
  "description": "IntLayer - Dictionaries Entry. This package only returns the dictionary entry path. For bundle configuration, this packages should be replaced by using an alias.",
  "keywords": [
    "intlayer",
    "alias",
    "dictionaries",
    "entry"
  ],
  "homepage": "https://github.com/aypineau/intlayer",
  "repository": {
    "type": "git",
    "url": "https://github.com/aypineau/intlayer"
  },
  "license": "MIT",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aypineau"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "module": "",
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ]
    }
  },
  "files": [
    "./dist",
    "./src",
    "./package.json"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "prettier:fix": "prettier --write **/*.{ts,tsx,js,json,md}",
    "process-files": "ts-node src/transpiler/processFilesCLI.ts --dir $npm_config_dir --extension $npm_config_extension --no-node-snapshot",
    "test": "",
    "typecheck": "tsup--project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@intlayer/config": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@utils/eslint-config": "workspace:^",
    "rimraf": "5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=14.18"
  },
  "bug": {
    "url": "https://github.com/aypineau/intlayer/issues"
  }
}
