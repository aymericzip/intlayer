{
  "name": "@intlayer/webpack",
  "version": "2.0.4",
  "private": false,
  "description": "Webpack application for IntLayer - Transpile Intlayer declaration files into dictionaries using webpack.",
  "keywords": [
    "intlayer",
    "webpack",
    "application",
    "transpile",
    "typescript",
    "javascript",
    "json",
    "file"
  ],
  "homepage": "https://github.com/aypineau/intlayer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aypineau/intlayer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aypineau"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ]
    }
  },
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "prettier:fix": "prettier --write src/**/*",
    "serve": "webpack serve --config ./webpack.config.ts",
    "transpile": "webpack --config ./webpack.config.ts",
    "typecheck": "tsup--project ./tsconfig.json --noEmit",
    "watch": "webpack --config ./webpack.config.ts --watch"
  },
  "dependencies": {
    "@intlayer/chokidar": "workspace:^",
    "@intlayer/cli": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "chokidar": "^3.6.0",
    "crypto-js": "^4.2.0",
    "esbuild": "^0.20.2",
    "esbuild-loader": "^4.2.0",
    "glob": "^10.4.2",
    "intlayer": "workspace:^",
    "node-loader": "^2.0.0",
    "rimraf": "5.0.5",
    "util": "^0.12.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.9",
    "@utils/eslint-config": "workspace:^",
    "@utils/ts-config": "workspace:^",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=14.18"
  },
  "bug": {
    "url": "https://github.com/aypineau/intlayer/issues"
  }
}
