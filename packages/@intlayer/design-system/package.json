{
  "name": "@intlayer/design-system",
  "version": "6.1.6",
  "private": false,
  "description": "Intlayer design system, including UI components used in the Intlayer editor, website, and visual editor/CMS.",
  "keywords": [
    "design-system",
    "intlayer",
    "react",
    "ui"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.cjs"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.cjs"
    },
    "./libs": {
      "types": "./dist/libs/index.d.ts",
      "import": "./dist/libs/index.mjs",
      "require": "./dist/libs/index.cjs"
    },
    "./tailwind-config": {
      "types": "./dist/tailwind.config.d.ts",
      "import": "./dist/tailwind.config.mjs",
      "require": "./dist/tailwind.config.cjs"
    },
    "./css": "./src/tailwind.css",
    "./css-output": "./dist/tailwind.css"
  },
  "main": "dist/components/index.cjs",
  "module": "dist/components/index.mjs",
  "types": "dist/components/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/components/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "providers": [
        "./dist/providers/index.d.ts"
      ],
      "libs": [
        "./dist/libs/index.d.ts"
      ],
      "tailwind-config": [
        "./dist/tailwind.config.d.ts"
      ],
      "package.json": [
        "./package.json"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:ci": "vite build",
    "build:css": "npx tailwindcss --minify -i ./src/tailwind.css -o ./dist/tailwind.css",
    "build:intlayer": "intlayer build",
    "build:package": "vite build",
    "build:storybook": "storybook build",
    "check:storybook": "npx storybook doctor",
    "clean": "rimraf ./dist .turbo",
    "dev": "vite build --watch",
    "format": "biome format . --check",
    "format:fix": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "preview": "vite preview",
    "prepublish": "cp -f ../../../README.md ./README.md",
    "start": "bun storybook -p 6006 --no-open --host 0.0.0.0",
    "start:serve": "npx http-server -y storybook-static -p 6006 -o --no-open",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@intlayer/api": "workspace:*",
    "@intlayer/config": "workspace:*",
    "@intlayer/core": "workspace:*",
    "@intlayer/dictionaries-entry": "workspace:*",
    "@intlayer/editor-react": "workspace:*",
    "@intlayer/types": "workspace:*",
    "@monaco-editor/react": "4.7.0",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-toast": "1.2.15",
    "@shikijs/transformers": "3.13.0",
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-query-devtools": "5.90.2",
    "better-auth": "1.3.27",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "framer-motion": "12.23.24",
    "fuse.js": "7.1.0",
    "intlayer": "workspace:*",
    "react-hook-form": "7.65.0",
    "react-intlayer": "workspace:*",
    "rollup-preserve-directives": "1.1.3",
    "shiki": "3.13.0",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.12",
    "zustand": "4.5.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.7",
    "@intlayer/backend": "workspace:*",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/cli": "4.0.14",
    "@tailwindcss/vite": "4.1.13",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/espree": "10.1.0",
    "@types/node": "24.7.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@utils/ts-config": "workspace:*",
    "@utils/ts-config-types": "workspace:*",
    "@vitejs/plugin-react": "5.0.4",
    "fast-glob": "3.3.3",
    "lucide-react": "0.546.0",
    "rimraf": "6.0.1",
    "storybook": "8.6.14",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3",
    "vite": "7.1.10",
    "vite-intlayer": "workspace:*",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@better-fetch/fetch": "1.1.18",
    "@intlayer/api": "workspace:*",
    "@intlayer/backend": "workspace:*",
    "@intlayer/config": "workspace:*",
    "@intlayer/core": "workspace:*",
    "@intlayer/dictionaries-entry": "workspace:*",
    "@intlayer/editor-react": "workspace:*",
    "@intlayer/types": "workspace:*",
    "@monaco-editor/react": "4.7.0",
    "@tanstack/react-query": "5.90.5",
    "clsx": "2.1.1",
    "framer-motion": "12.23.24",
    "fuse.js": "7.1.0",
    "intlayer": "workspace:*",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "react-intlayer": "workspace:*",
    "tailwind-merge": "3.3.1",
    "zustand": "4.5.6"
  }
}
