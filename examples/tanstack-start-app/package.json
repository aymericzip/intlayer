{
  "name": "tanstack-start-intlayer",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node dist/server/server.js",
    "build": "vite build && cp -r .intlayer .output/.intlayer",
    "serve": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --project tsconfig.types.json",
    "start:editor": "npx intlayer-editor start -e development",
    "intlayer:build": "intlayer build"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.16",
    "@tanstack/react-router": "1.143.11",
    "@tanstack/react-router-devtools": "1.143.11",
    "@tanstack/react-router-ssr-query": "1.143.11",
    "@tanstack/react-start": "1.143.12",
    "@tanstack/router-plugin": "1.143.11",
    "intlayer": "workspace:*",
    "nitro": "latest",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-intlayer": "workspace:*",
    "tailwindcss": "4.1.18",
    "vite-tsconfig-paths": "6.0.3"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.1",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "jsdom": "27.2.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-intlayer": "workspace:*",
    "vitest": "4.0.17",
    "web-vitals": "5.1.0"
  }
